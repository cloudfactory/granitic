{
  "QueryManager":{
    "TemplateLocation": "resource/queries",
    "QueryIDPrefix": "ID:",
    "TrimIDWhiteSpace": true,
    "VarMatchRegEx": "\\$\\{([^\\}]*)\\}",
    "NewLine": "\n",
    "CreateDefaultValueProcessor": true,
    "ProcessorName": "configurable",
    "ValueProcessors": {
      "Configurable": {
        "WrapStrings": true,
        "StringWrapWith": "'"
      },
      "SQL": {
        "BoolFalse": 0,
        "BoolTrue": 1
      }
    }
  }
}